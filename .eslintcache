     <Menu.Item>English</Menu.Item> */}\r\n                  {this.state.languageList.map(l => {\r\n                    return <Menu.Item key={l.code}>{l.name}</Menu.Item>\r\n                  })}\r\n                </Menu>\r\n              }\r\n              icon={<GlobalOutlined />}\r\n            >\r\n              {this.state.language == 'zh' ? '中文' : 'EN'}\r\n            </Dropdown.Button>\r\n            <Button.Group className={styles['button-group']}>\r\n              <Button onClick={() => history.push('register')}>注册</Button>\r\n              <Button onClick={() => history.push('signIn')}>登陆</Button>\r\n              <Button onClick={() => history.push('schedule')}>赛事</Button>\r\n              <Button onClick={() => history.push('ScheduleMsg')}>赛程</Button>\r\n              <Button onClick={() => history.push('WonderfulReplay')}>赛事回放</Button>\r\n            </Button.Group>\r\n          </div>\r\n        </div>\r\n        <Layout.Header className={styles['main-header']}>\r\n          <span onClick={() => history.push('/')}>\r\n            <img src={logo} alt='logo' className={styles['App-logo']} />\r\n            <Typography.Title level={3} className={styles.title}>\r\n              React旅游网\r\n            </Typography.Title>\r\n          </span>\r\n          <Input.Search\r\n            placeholder={'请输入旅游目的地、主题、或关键字'}\r\n            className={styles['search-input']}\r\n          />\r\n        </Layout.Header>\r\n        <Menu mode={'horizontal'} className={styles['main-menu']}>\r\n          <Menu.Item key={1}>旅游首页</Menu.Item>\r\n          <Menu.Item key={2}>周末游</Menu.Item>\r\n          <Menu.Item key={3}>跟团游</Menu.Item>\r\n          <Menu.Item key='4'> 自由行 </Menu.Item>\r\n          <Menu.Item key='5'> 私家团 </Menu.Item>\r\n          <Menu.Item key='6'> 邮轮 </Menu.Item>\r\n          <Menu.Item key='7'> 酒店+景点 </Menu.Item>\r\n          <Menu.Item key='8'> 当地玩乐 </Menu.Item>\r\n          <Menu.Item key='9'> 主题游 </Menu.Item>\r\n          <Menu.Item key='10'> 定制游 </Menu.Item>\r\n          <Menu.Item key='11'> 游学 </Menu.Item>\r\n          <Menu.Item key='12'> 签证 </Menu.Item>\r\n          <Menu.Item key='13'> 企业游 </Menu.Item>\r\n          <Menu.Item key='14'> 高端游 </Menu.Item>\r\n          <Menu.Item key='15'> 爱玩户外 </Menu.Item>\r\n          <Menu.Item key='16'> 保险 </Menu.Item>\r\n        </Menu>\r\n      </div>\r\n    )\r\n  }\r\n}\r\n\r\nexport const Header = withRouter(HeaderComponnet) //高阶组件中的withRouter, 作用是将一个组件包裹进Route里面, 然后react-router的三个对象history, location, match就会被放进这个组件的props属性中\r\n","G:\\ts-width-react\\src\\components\\sideMenu\\SideMenu.tsx",[],"G:\\ts-width-react\\src\\components\\productCollection\\ProductCollection.tsx",["256"],"import React from \"react\";\r\nimport styles from \"./ProductCollection.module.css\";\r\nimport { Row, Col, Typography, Divider } from \"antd\";\r\nimport { ProductImage } from \"./ProductImage\";\r\n\r\ninterface PropsType {\r\n    title: JSX.Element;\r\n    sideImage: string;\r\n    products: any[];\r\n}\r\n\r\nexport const ProductCollection: React.FC<PropsType> = ({title, sideImage, products}) => {\r\n    return (\r\n      <div className={styles.content}>\r\n        <Divider orientation=\"left\">{title}</Divider>\r\n        <Row>\r\n          <Col span={4}>\r\n            <img src={sideImage} className={styles[\"side-image\"]} alt=\"\" />\r\n          </Col>\r\n          <Col span={20}>\r\n            <Row>\r\n              <Col span={12}>\r\n                <ProductImage\r\n                  id={products[0].id}\r\n                  size={\"large\"}\r\n                  title={products[0].title}\r\n                  imageSrc={products[0].touristRoutePictures[0].url}\r\n                  price={products[0].price}\r\n                />\r\n              </Col>\r\n              <Col span={12}>\r\n                <Row>\r\n                  <Col span={12}>\r\n                    <ProductImage\r\n                      id={products[1].id}\r\n                      size=\"small\"\r\n             